  {
   "cell_type": "code",
   "execution_count": 66,
   "id": "733da216-c016-46e6-b53d-45951e562738",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-08-01T04:58:10.059262Z",
     "iopub.status.busy": "2024-08-01T04:58:10.058499Z",
